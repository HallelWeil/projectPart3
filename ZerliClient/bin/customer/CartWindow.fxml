<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="cartPane" prefHeight="695.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <AnchorPane prefHeight="95.0" prefWidth="300.0">
         <children>
            <Label fx:id="cartLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="104.0" layoutY="55.0" text="My Cart" textAlignment="CENTER">
               <font>
                  <Font name="System Bold Italic" size="24.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <AnchorPane prefHeight="483.0" prefWidth="300.0">
         <children>
            <VBox fx:id="cartItemsBox" layoutX="10.0" layoutY="7.0" prefHeight="450.0" prefWidth="280.0" style="-fx-border-color: black;" />
         </children>
      </AnchorPane>
      <AnchorPane prefHeight="200.0" prefWidth="200.0">
         <children>
            <Button fx:id="placeOrderBtn" contentDisplay="TOP" layoutX="95.0" layoutY="34.0" mnemonicParsing="false" onAction="#PalceOrder" prefHeight="35.0" prefWidth="111.0" style="-fx-background-color: #2f41a7;" text="Place Order" textFill="WHITE">
               <font>
                  <Font name="System Bold Italic" size="12.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</VBox>
